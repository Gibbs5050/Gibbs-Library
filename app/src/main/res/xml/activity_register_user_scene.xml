<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="756dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="373dp"
            android:id="@+id/imageView6" />
        <Constraint
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="20dp"
            motion:layout_constraintBottom_toTopOf="@+id/passwordRegisterText"
            motion:layout_constraintHorizontal_bias="0.597"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="319dp"
            android:id="@+id/emailRegisterText" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="733dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="347dp"
            android:id="@+id/imageView6" />
        <Constraint
            android:layout_height="50dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="20dp"
            motion:layout_constraintBottom_toTopOf="@+id/passwordRegisterText"
            motion:layout_constraintHorizontal_bias="0.597"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="319dp"
            android:id="@+id/emailRegisterText" />
        <Constraint
            motion:layout_editor_absoluteY="453dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/registerButton"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:layout_height="50dp"
            android:layout_marginTop="88dp"
            motion:layout_constraintHorizontal_bias="0.597"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="319dp"
            motion:layout_constraintTop_toBottomOf="@+id/ageRegisterText"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/passwordRegisterText" />
        <Constraint
            android:layout_height="56dp"
            android:layout_marginBottom="52dp"
            motion:layout_constraintBottom_toTopOf="@+id/editRegisterSignIn"
            android:layout_width="290dp"
            motion:layout_constraintStart_toEndOf="@+id/checkBoxTerms"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:id="@+id/termsAndConditions" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_marginBottom="152dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:id="@+id/checkBoxTerms" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="76dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/editRegisterSignIn" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="36dp"
            motion:layout_constraintBottom_toTopOf="@+id/nameRegisterText"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="265dp"
            android:id="@+id/TitleRegisterPage" />
        <Constraint
            android:layout_height="50dp"
            android:layout_width="43dp"
            android:id="@+id/progressBar2"
            motion:layout_constraintEnd_toEndOf="@+id/imageView6"
            motion:layout_constraintStart_toStartOf="@+id/imageView6"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="652dp"
            motion:layout_constraintHorizontal_bias="0.312"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView6" />
    </ConstraintSet>
</MotionScene>